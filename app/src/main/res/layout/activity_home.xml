<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="edu.umich.andrewbull.piedpiper.HomeActivity"
    tools:layout_editor_absoluteY="81dp"
    tools:layout_editor_absoluteX="0dp">

    <ImageButton
        android:id="@+id/imageButtonDish1"
        android:layout_width="87dp"
        android:layout_height="0dp"
        android:background="@mipmap/pizza_icon"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/searchView"
        app:layout_constraintLeft_toLeftOf="@+id/guideline2" />

    <ImageButton
        android:id="@+id/imageButtonDish2"
        android:layout_width="86dp"
        android:layout_height="wrap_content"
        android:background="@mipmap/pasta_icon"
        app:layout_constraintLeft_toRightOf="@+id/imageButtonDish1"
        android:layout_marginLeft="5dp"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/searchView"
        android:layout_marginStart="5dp" />

    <ImageButton
        android:id="@+id/imageButtonNoodles"
        android:layout_width="86dp"
        android:layout_height="wrap_content"
        android:background="@mipmap/noodles_icon"
        app:layout_constraintLeft_toRightOf="@+id/imageButtonDish1"
        android:layout_marginLeft="8dp"
        app:layout_constraintRight_toRightOf="@+id/imageButtonDish2"
        app:layout_constraintTop_toTopOf="@+id/textViewDish2"
        android:layout_marginTop="49dp"
        app:layout_constraintHorizontal_bias="0.0"
        android:layout_marginStart="8dp" />

    <ImageButton
        android:id="@+id/imageButtonDish3"
        android:layout_width="86dp"
        android:layout_height="wrap_content"
        android:background="@mipmap/sandwich_icon"
        app:layout_constraintLeft_toRightOf="@+id/imageButtonDish2"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/searchView"
        android:layout_marginStart="8dp" />

    <ImageButton
        android:id="@+id/imageButtonTaco"
        android:layout_width="86dp"
        android:layout_height="wrap_content"
        android:background="@mipmap/taco_icon"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="@+id/imageButtonDish3"
        app:layout_constraintRight_toRightOf="@+id/imageButtonDish3"
        app:layout_constraintTop_toTopOf="@+id/textViewDish3"
        android:layout_marginTop="49dp"
        app:layout_constraintHorizontal_bias="1.0"
        android:layout_marginStart="8dp" />

    <ImageButton
        android:id="@+id/imageButtonDish4"
        android:layout_width="87dp"
        android:layout_height="wrap_content"
        android:background="@mipmap/burger_icon"
        app:layout_constraintLeft_toRightOf="@+id/imageButtonDish3"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/searchView"
        android:layout_marginStart="8dp" />

    <ImageButton
        android:id="@+id/imageButtonSalad"
        android:layout_width="86dp"
        android:layout_height="wrap_content"
        android:background="@mipmap/salad_icon"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="@+id/imageButtonDish4"
        app:layout_constraintRight_toRightOf="@+id/imageButtonDish4"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintTop_toTopOf="@+id/textViewDish4"
        android:layout_marginTop="49dp"
        android:layout_marginStart="8dp" />

    <TextView
        android:id="@+id/textViewDish1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Pizza"
        app:layout_constraintTop_toBottomOf="@+id/imageButtonDish1"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="@+id/imageButtonDish1"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="@+id/imageButtonDish1"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp" />

    <ImageButton
        android:id="@+id/imageButtonSushi"
        android:layout_width="0dp"
        android:layout_height="48dp"
        android:background="@mipmap/sushi_icon"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/imageButtonDish1"
        android:layout_marginTop="49dp"
        app:layout_constraintTop_toBottomOf="@+id/imageButtonDish1"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/imageButtonDish1"
        app:layout_constraintHorizontal_bias="0.0" />

    <TextView
        android:id="@+id/textViewSushi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Sushi"
        app:layout_constraintTop_toBottomOf="@+id/imageButtonSushi"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="@+id/imageButtonSushi"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="@+id/imageButtonSushi"
        app:layout_constraintHorizontal_bias="0.405"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp" />

    <TextView
        android:id="@+id/textViewDish2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        app:layout_constraintTop_toBottomOf="@+id/imageButtonDish2"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="@+id/imageButtonDish2"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="@+id/imageButtonDish2"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp" />

    <TextView
        android:id="@+id/textViewNoodles"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Noodles"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="@+id/imageButtonNoodles"
        android:layout_marginLeft="8dp"
        app:layout_constraintHorizontal_bias="0.388"
        app:layout_constraintLeft_toLeftOf="@+id/imageButtonNoodles"
        app:layout_constraintTop_toBottomOf="@+id/imageButtonNoodles"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp" />

    <TextView
        android:id="@+id/textViewDish3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        app:layout_constraintTop_toBottomOf="@+id/imageButtonDish3"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="@+id/imageButtonDish3"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="@+id/imageButtonDish3"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp" />

    <TextView
        android:id="@+id/textViewTacos"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Tacos"
        app:layout_constraintTop_toBottomOf="@+id/imageButtonTaco"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="@+id/imageButtonTaco"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="@+id/imageButtonTaco"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp" />

    <TextView
        android:id="@+id/textViewDish4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        app:layout_constraintTop_toBottomOf="@+id/imageButtonDish4"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="@+id/imageButtonDish4"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="@+id/imageButtonDish4"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp" />

    <TextView
        android:id="@+id/textViewSalad"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Salad"
        android:layout_marginTop="5dp"
        app:layout_constraintTop_toBottomOf="@+id/imageButtonSalad"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="@+id/imageButtonSalad"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="@+id/imageButtonSalad"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp" />

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline"
        app:layout_constraintGuide_begin="20dp"
        android:orientation="vertical"
        tools:layout_editor_absoluteY="0dp"
        tools:layout_editor_absoluteX="20dp" />

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline2"
        app:layout_constraintGuide_begin="20dp"
        android:orientation="vertical"
        tools:layout_editor_absoluteY="0dp"
        tools:layout_editor_absoluteX="20dp" />

    <SearchView
        android:id="@+id/searchView"
        android:layout_width="390dp"
        android:layout_height="72dp"
        tools:layout_editor_absoluteY="0dp"
        tools:layout_editor_absoluteX="0dp" />

    <ListView
        android:id="@+id/searchSuggestionsListView"
        android:layout_width="371dp"
        android:layout_height="200dp"
        app:layout_constraintTop_toBottomOf="@+id/searchView"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent" />

</android.support.constraint.ConstraintLayout>