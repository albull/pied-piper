<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="edu.umich.andrewbull.piedpiper.HomeActivity">

    <ImageButton
        android:id="@+id/imageButtonDish1"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:srcCompat="@mipmap/ic_launcher"
        app:layout_constraintRight_toLeftOf="@+id/imageButtonDish2"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        android:layout_marginStart="12dp"
        android:layout_marginEnd="4dp"
        android:layout_marginTop="68dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginLeft="12dp"
        android:layout_marginRight="4dp"
        app:layout_constraintHorizontal_bias="1.0" />

    <ImageButton
        android:id="@+id/imageButtonDish2"
        android:layout_width="86dp"
        android:layout_height="wrap_content"
        app:srcCompat="@mipmap/ic_launcher"
        app:layout_constraintBottom_toBottomOf="@+id/imageButtonDish1"
        app:layout_constraintTop_toTopOf="@+id/imageButtonDish1"
        tools:layout_constraintRight_creator="1"
        android:layout_marginStart="102dp"
        android:layout_marginEnd="92dp"
        app:layout_constraintRight_toRightOf="@+id/imageButtonDish3"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="102dp"
        android:layout_marginRight="92dp" />

    <ImageButton
        android:id="@+id/imageButtonDish3"
        android:layout_width="86dp"
        android:layout_height="wrap_content"
        app:srcCompat="@mipmap/ic_launcher"
        app:layout_constraintBottom_toBottomOf="@+id/imageButtonDish2"
        app:layout_constraintTop_toTopOf="@+id/imageButtonDish2"
        tools:layout_constraintRight_creator="1"
        android:layout_marginEnd="104dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginRight="104dp"
        app:layout_constraintVertical_bias="1.0" />

    <ImageButton
        android:id="@+id/imageButtonDish4"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:srcCompat="@mipmap/ic_launcher"
        app:layout_constraintBottom_toBottomOf="@+id/imageButtonDish3"
        app:layout_constraintTop_toTopOf="@+id/imageButtonDish3"
        tools:layout_constraintRight_creator="1"
        android:layout_marginStart="6dp"
        android:layout_marginEnd="12dp"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toRightOf="@+id/imageButtonDish3"
        android:layout_marginLeft="6dp"
        android:layout_marginRight="12dp" />

    <TextView
        android:id="@+id/textViewDish1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Pizza"
        app:layout_constraintLeft_toLeftOf="@+id/imageButtonDish1"
        app:layout_constraintRight_toRightOf="@+id/imageButtonDish1"
        app:layout_constraintTop_toTopOf="@+id/textViewDish2" />

    <TextView
        android:id="@+id/textViewDish2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        app:layout_constraintTop_toTopOf="@+id/textViewDish3"
        app:layout_constraintRight_toRightOf="@+id/imageButtonDish2"
        app:layout_constraintLeft_toLeftOf="@+id/imageButtonDish2" />

    <TextView
        android:id="@+id/textViewDish3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        app:layout_constraintTop_toTopOf="@+id/textViewDish4"
        app:layout_constraintRight_toRightOf="@+id/imageButtonDish3"
        app:layout_constraintLeft_toLeftOf="@+id/imageButtonDish3" />

    <TextView
        android:id="@+id/textViewDish4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/imageButtonDish4"
        app:layout_constraintRight_toRightOf="@+id/imageButtonDish4"
        app:layout_constraintLeft_toLeftOf="@+id/imageButtonDish4" />

    <ListView
        android:id="@+id/searchSuggestionsListView"
        android:layout_width="371dp"
        android:layout_height="200dp"
        app:layout_constraintTop_toBottomOf="@+id/searchView"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent" />

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline"
        app:layout_constraintGuide_begin="20dp"
        android:orientation="vertical"
        tools:layout_editor_absoluteY="0dp"
        tools:layout_editor_absoluteX="20dp" />

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline2"
        app:layout_constraintGuide_begin="20dp"
        android:orientation="vertical"
        tools:layout_editor_absoluteY="0dp"
        tools:layout_editor_absoluteX="20dp" />

    <SearchView
        android:id="@+id/searchView"
        android:layout_width="390dp"
        android:layout_height="72dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_editor_absoluteY="0dp" />

</android.support.constraint.ConstraintLayout>
