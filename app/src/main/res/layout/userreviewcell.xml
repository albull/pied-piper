<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="match_parent">

    <TextView
        android:id="@+id/userReviewRestaurantNameTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="23dp"
        android:layout_marginStart="23dp"
        android:layout_marginTop="11dp"
        android:text="TextView"
        android:textSize="16sp" />

    <TextView
        android:id="@+id/userReviewDishNameTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:text="TextView"
        android:textSize="16sp"
        android:layout_below="@+id/userReviewRestaurantReviewTextView"
        android:layout_alignLeft="@+id/userReviewRestaurantReviewTextView"
        android:layout_alignStart="@+id/userReviewRestaurantReviewTextView" />

    <TextView
        android:id="@+id/userReviewRestaurantReviewTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:layout_below="@+id/userReviewRestaurantNameTextView"
        android:layout_alignLeft="@+id/userReviewRestaurantNameTextView"
        android:layout_alignStart="@+id/userReviewRestaurantNameTextView"
        android:layout_marginTop="11dp" />

    <TextView
        android:id="@+id/userReviewDishReviewTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/userReviewDishNameTextView"
        android:layout_alignStart="@+id/userReviewDishNameTextView"
        android:layout_below="@+id/userReviewDishNameTextView"
        android:layout_marginTop="14dp"
        android:text="TextView" />

    <TextView
        android:id="@+id/userReviewRestaurantRatingTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignTop="@+id/userReviewRestaurantNameTextView"
        android:layout_marginEnd="50dp"
        android:layout_marginRight="50dp"
        android:text="TextView"
        android:textSize="16sp" />

    <TextView
        android:id="@+id/userReviewDishRatingTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/userReviewRestaurantRatingTextView"
        android:layout_alignStart="@+id/userReviewRestaurantRatingTextView"
        android:layout_alignTop="@+id/userReviewDishNameTextView"
        android:text="TextView"
        android:textSize="16sp" />
</RelativeLayout>